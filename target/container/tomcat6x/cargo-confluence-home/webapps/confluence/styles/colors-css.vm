#disableAntiXss()

#set($successText = "#008004")
#set($alertText = "#cc0000")

/*
    Colors for Confluence (included for all themes by default).
*/
h1, h2, h3, h4, h5, h6,
.wiki-content h1,
.wiki-content h2,
.wiki-content h3,
.wiki-content h4,
.wiki-content h5,
.wiki-content h6,
.pagetitle,
.steptitle,
.substeptitle,
.formtitle,
table.confluenceTable td.confluenceTd.highlight,
table.confluenceTable td.confluenceTd.highlight > p,
table.confluenceTable th.confluenceTh,
table.confluenceTable th.confluenceTh > p,
table.admin th,
.form-element-large,
.form-element-small,
#toolbar #format-dropdown .format-h1 a,
#toolbar #format-dropdown .format-h2 a,
#toolbar #format-dropdown .format-h3 a,
#toolbar #format-dropdown .format-h4 a,
#toolbar #format-dropdown .format-h5 a,
#toolbar #format-dropdown .format-h6 a {
    color: $colorScheme.headingTextColor;
}

#format-dropdown .mce_formatPreview.mce_blockquote a {
    border-left-color: $colorScheme.borderColor;
}

.wiki-content h1,
.wiki-content h2 {
 	border-bottom-color: $colorScheme.borderColor;
}

.wiki-content-preview {
    border-left-color: $colorScheme.borderColor;
    border-right-color: $colorScheme.borderColor;
}

.pageSectionHeader {
    border-bottom-color: $colorScheme.navBackgroundColor;
}

.panel {
    border-color: $colorScheme.borderColor;
}

.panelHeader,
.menuheading,
.pageheader,
.sectionbottom {
    border-bottom-color: $colorScheme.borderColor;
}

.topRow {
    border-top-color: $colorScheme.borderColor;
}

.tabletitle, .pageSectionHeader {
	border-bottom-color: $colorScheme.borderColor;
}

a:link,
a:visited,
a:focus,
a:hover,
a:active,
a.blogHeading {
	color: $colorScheme.linkColor;
}

.wiki-content a,
.wiki-content a:link,
.wiki-content a:visited,
.wiki-content a:focus,
.wiki-content a:hover,
.wiki-content a:active {
	color: $colorScheme.linkColor;
}

.createlink,
a.createlink,
a.createlink:link,
a.createlink:visited,
a.createlink:active,
a.createlink:hover {
    text-decoration: underline;
    color: #6D1806;
}

.unresolved,
a.unresolved,
a.unresolved:link,
a.unresolved:visited,
a.unresolved:active,
a.unresolved:hover {
    text-decoration: underline;
    color: #828282;
}

.not-permitted,
a.not-permitted,
a.not-permitted:link,
a.not-permitted:visited,
a.not-permitted:hover,
a.not-permitted:active {
    color: $alertText;
}

.grey,
a.grey,
a.grey:link,
a.grey:visited,
a.grey:hover,
a.grey:active {
    color: #666;
}

#tinymce.wiki-content a,
#tinymce.wiki-content a:link,
#tinymce.wiki-content a:visited,
#tinymce.wiki-content a:focus,
#tinymce.wiki-content a:hover,
#tinymce.wiki-content a:active {
	color: $colorScheme.linkColor !important;
}

#tinymce.wiki-content a.createlink {
    color: #6D1806  !important; /* !important used to negate styles used in designMode in FF */
}

#tinymce.wiki-content a.unresolved {
    color: #828282 !important; /* !important used to negate styles used in designMode in FF */
}

#tinymce.wiki-content a.not-permitted {
    color: $alertText !important; /* !important used to negate styles used in designMode in FF */
}

h1 a:link,
h1 a:visited,
h1 a:focus,
h1 a:hover,
h1 a:active {
    color: $colorScheme.linkColor;
    border-bottom-color: $colorScheme.headingTextColor;
}

.spacenametitle-printable,
.spacenametitle-printable a,
.spacenametitle-printable a:visited {
    color: $colorScheme.spaceNameColor;
}

.tableview th {
    color: $colorScheme.headingTextColor;
    border-bottom-color: $colorScheme.borderColor;
}

blockquote {
    border-left-color: $colorScheme.borderColor;
}

.navBackgroundBox {
    background: $colorScheme.navBackgroundColor;
}

.previewBoxTop,
.previewContent,
.previewBoxBottom,
.functionbox {
    border-color: $colorScheme.borderColor;
}

.smalltext-blue {
    color: $colorScheme.borderColor;
}

.tabnav,
.comment .tabnav,
ul.tabnav {
    border-bottom-color: $colorScheme.navBackgroundColor;
}

.tabnav .tabs a {
    border-color: $colorScheme.navBackgroundColor;
    background: $colorScheme.navBackgroundColor;
}

.tabnav .tabs a:link,
.tabnav .tabs a:visited {
    color: $colorScheme.navTextColor;
}
.tabnav .tabs a:hover {
    color: $colorScheme.navSelectedTextColor;
    background: $colorScheme.navSelectedBackgroundColor;
    border-color: $colorScheme.navSelectedBackgroundColor;
}

#squaretab a {
    border-color: $colorScheme.navBackgroundColor;
}

#squaretab a:link,
#squaretab a:visited {
    background-color: $colorScheme.navBackgroundColor;
}

#squaretab a:hover {
    color: $colorScheme.navSelectedTextColor;
    background-color: $colorScheme.navSelectedBackgroundColor;
    border-color: $colorScheme.navSelectedBackgroundColor;
}

table.blogcalendar {
    border-color: $colorScheme.borderColor;
}

.blogcalendar th.calendarhead,
a.calendarhead,
a.calendarhead:link,
a.calendarhead:visited,
a.calendarhead:hover {
    background-color: $colorScheme.navBackgroundColor;
    color: $colorScheme.navTextColor;
}

.searchGroupHeading {
    background-color: $colorScheme.navBackgroundColor;
    color: $colorScheme.navTextColor;
}

.permissionTab {
    background: $colorScheme.navBackgroundColor;
    color: $colorScheme.navTextColor;
}

.permissionSuperTab {
    background: $colorScheme.navSelectedBackgroundColor;
    color: $colorScheme.navSelectedTextColor;
}

/* styles for links in the top bar */
.topBar {
    background-color: $colorScheme.topBarColor;
}

.basicPanelContainer {
    border-color: $colorScheme.navBackgroundColor;
}

.greynavbar {
    border-top-color: $colorScheme.navBackgroundColor
}

div.license-personal {
   background-color: $colorScheme.topBarColor;
   color: $colorScheme.navTextColor;
}

div.license-personal a {
   color: $colorScheme.navTextColor;
}

#header {
    background-color: $colorScheme.topBarColor;
}

#header a,
\#breadcrumbs {
    color: $colorScheme.breadcrumbsTextColor;
}
.breadcrumbs {
 	border-color: $colorScheme.borderColor;
}

#navigation, #tab-navigation {
    border-bottom-color: $colorScheme.navBackgroundColor;
}

.tab-navigation .tab a {
    background-color: $colorScheme.navBackgroundColor;
    border: 1px solid $colorScheme.navBackgroundColor;
    color: $colorScheme.navTextColor;
}
.tab-navigation .tab a:hover {
    color: $colorScheme.navSelectedTextColor;
    background-color: $colorScheme.navSelectedBackgroundColor;
    border-bottom-color: $colorScheme.navSelectedBackgroundColor;
}

.tabletitle, .heading-text-color {
    color: $colorScheme.headingTextColor;
}

ol.autocompleter li.focused {
	background: $colorScheme.menuSelectedBackgroundColor;
	color: $colorScheme.menuItemSelectedTextColor;
}

/* End colour styles for Confluence */

.diff-navigation a.version-navigation {
    background-color: #ddd;
}

.diff-navigation a.version-navigation:hover {
    background-color: #bbb;
}

.diff-navigation a.version-navigation.all:hover {
    background-color: transparent;
}

/* @group Admin tasklist colours */

.tasklist .task.complete .signoff { color: $successText; }
.tasklist .task.incomplete .signoff { color: $alertText; }

/* @end Admin tasklist colours */

#parse('/styles/aui-overrides-css.vm')

#parse('/styles/labels-css.vm')
#parse('/styles/diffs-css.vm')
