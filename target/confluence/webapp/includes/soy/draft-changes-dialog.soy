{namespace Confluence.Templates.DraftChanges}

/**
 * The dialog content for draft changes/diff on the edit page
 */
{template .dialogContent}
<div id="draft-changes-dialog" class="hidden">
    <div id="diff-view"></div>
</div>
{/template}

/**
 * Template for the help link on the draft changes dialog
 */
{template .helpLink}
   {call Confluence.Templates.Dialog.helpLink}
       {param href: docLink('help.view.changes.dialog') /}
   {/call}
{/template}
